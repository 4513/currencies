{
    "name": "mibo/currencies",
    "type": "library",
    "description": "PHP Library of ISO-4217 standard",
    "keywords": [
        "ISO",
        "ISO-4217",
        "currencies",
        "currency",
        "MiBo",
        "Library",
        "PHP",
        "Package",
        "Lib",
        "Money"
    ],
    "authors": [
        {
            "name": "Michal Boris"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MiBo\\Currencies\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\Currencies\\Tests\\": [
                "tests/Core/",
                "tests/"
            ]
        }
    },
    "extra": {
        "meta": {
            "installable": true,
            "section": "",
            "name": ""
        }
    },
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-xmlreader": "*",
        "ext-simplexml": "*",
        "psr/log": "^3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "phpunit/php-invoker": "^5.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.9"
    },
    "config": {
        "platform-check": true,
        "lock": false
    }
}
